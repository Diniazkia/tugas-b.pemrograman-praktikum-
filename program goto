#include <iostream>
using namespace std;

int main(){
	//program goto
	char jump = 'Z' ;
	
	cout << " masukkan D untuk melompat " << endl;
	cout << " masukkan N untuk tidak melompat " << endl;
	cin >> jump;
	
	cout << "E" <<endl;
	if (jump =='D') goto lompat;
	cout << "F" << endl;
	lompat:
	cout << "G" << endl;
	
	
	return 0;
}
